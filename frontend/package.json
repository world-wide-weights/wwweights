{
  "name": "wwweights-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently npm:dev-next npm:mock-server",
    "dev-next": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "npx cypress open",
    "test:cli": "npx cypress run",
    "test-components:cli": "npx cypress run --component",
    "mock-server": "json-server --watch ./src/mock/db.json -m ./node_modules/json-server-auth --port=3004 --routes ./src/mock/routes.json --id slug"
  },
  "dependencies": {
    "@next/font": "^13.0.3",
    "formik": "^2.2.9",
    "material-symbols": "^0.4.0",
    "next": "^13.0.3",
    "next-auth": "^4.18.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cypress": "^11.0.1",
    "eslint": "^8.27.0",
    "eslint-config-next": "^13.0.3",
    "json-server": "^0.17.1",
    "json-server-auth": "^2.1.0",
    "nock": "^13.2.9",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.3",
    "typescript": "^4.9.3"
  }
